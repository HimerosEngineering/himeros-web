<h1>Members</h1>

<table class='table table-bodered table-striped'>
  <thead>
    <tr>
      <th>#</th>
      <th>Nick Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <%= for user <- @users do %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.nick_name %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>